<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 为透明 -->
    <solid android:color="#00FFFFFF" />

    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="6dp" />

    <!-- 设置几何形状绘制边框 红色边框 -->
    <stroke
        android:width="1dp"
        android:color="#FED1C9C9" />

</shape>